{
  "name": "nr-social-hub",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "analyze": "webpack-bundle-analyzer ../www/stats.json",
    "update:check": "ng update && npm outdated",
    "build": "ng build --statsJson --base-href . --output-path ../www/",
    "prod": "ng build --prod --aot --base-href . --output-path ../www/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngsw:make": "node_modules\\.bin\\ngsw-config ..\\www .\\ngsw-config.json && copy node_modules\\@angular\\service-worker\\ngsw-worker.js ..\\www",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.6",
    "@angular/cdk": "^9.0.0-rc.5",
    "@angular/common": "~9.0.0-rc.6",
    "@angular/compiler": "~9.0.0-rc.6",
    "@angular/core": "~9.0.0-rc.6",
    "@angular/fire": "^5.2.3",
    "@angular/forms": "~9.0.0-rc.6",
    "@angular/material": "^9.0.0-rc.5",
    "@angular/platform-browser": "~9.0.0-rc.6",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.6",
    "@angular/router": "~9.0.0-rc.6",
    "@angular/service-worker": "~9.0.0-rc.6",
    "angular-mentions": "^1.1.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "crypto-js": "^3.1.9-1",
    "electron-push-receiver": "^2.1.3",
    "firebase": "^7.5.2",
    "jquery": "^3.4.1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "ngx-infinite-scroll": "^8.0.1",
    "pulltorefreshjs": "^0.1.20",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.5",
    "@angular/cli": "~9.0.0-rc.5",
    "@angular/compiler-cli": "~9.0.0-rc.6",
    "@angular/language-service": "~9.0.0-rc.6",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@types/cordova": "0.0.34",
    "@types/electron": "^1.6.10",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.4.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash": "^4.14.149",
    "@types/ng-cordova": "0.0.14",
    "@types/node": "^12.12.17",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.15.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "^8.5.4",
    "tslint": "~5.20.1",
